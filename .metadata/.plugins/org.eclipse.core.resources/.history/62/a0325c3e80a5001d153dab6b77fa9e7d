package jlox;

// Expr is the base class that all expression classes inherit from. 
abstract class Expr {
	static class Binary extends Expr {
		final Expr left;
		final Token operator;
		final Expr right;
		
		Binary(Expr left, Token operator, Expr right) {
			this.left = left;
			this.operator = operator;
			this.right = right;
		}
	}
}